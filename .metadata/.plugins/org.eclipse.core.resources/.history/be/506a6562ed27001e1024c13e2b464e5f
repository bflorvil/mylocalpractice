package Practice;

public class MathTests {

	public static void main(String[] args) {
		float z = 15.67f;
		double d = 67.87;
		int x = 10;
		int y = 3;
		System.out.println(x/y);
		System.out.println(x % y);
		System.out.println(z/y); // float / int gives float
		System.out.println(d/z); // float / double gives double

	}

}
