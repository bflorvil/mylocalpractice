package Practice;
public class PrintStatements {
	int a;

	public static void main(String[] args) {
		int y = 40;
		String[] myStrings = new String[5];
		String x = new String("5");
		System.out.print("Hello World!\nHow are you?");
	    System.out.print("\nI'm fine thanks for asking");
	    System.out.println("\n"+5+"\t"+4);
	    attempt();
	    System.out.println(x + " is the value of x");
	    //////////////////////////////Converting between integers and strings////////////////////////////////////////////////////////
	    System.out.println(Integer.parseInt(x) * 7); //converts string value to an integer value so you can do arithmetic on it

	    //String.valueOf(integer i) OR Integer.toString(integer i) to convert integer i to string
 	    myStrings[0] = Integer.toString(y); //need to convert integer to string to put into string array
	    System.out.println(myStrings[0]);
	    
	    PrintStatements ps = new PrintStatements(y);
	   

	}
	public static String attempt() {
	    String y;
		return y = (5 > 4) ? "Yes" : "No"; //ignores assignment to y... return (5 > 4) ? "Yes" : "No"
		
	}
	
	public PrintStatements(int a) {
		System.out.println(a);
		System.out.println(this.a);
		
	}
		
	}
	
	
	
}
